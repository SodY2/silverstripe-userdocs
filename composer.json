{
  "name": "vulcandigital/silverstripe-userdocs",
  "description": "A module for creating user documentation",
  "type": "silverstripe-vendormodule",
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Reece Alexander",
      "email": "reece@vulcandigital.co.nz"
    }
  ],
  "require": {
    "silverstripe/recipe-cms": "^1",
    "sunra/php-simple-html-dom-parser": "^1.5.2",
    "symbiote/silverstripe-gridfieldextensions": "^3"
  },
  "require-dev": {
    "phpunit/PHPUnit": "^5.7",
    "squizlabs/php_codesniffer": "3.*"
  },
  "suggest": {
    "vulcandigital/silverstripe-pagefeedback": "This module allows you to receive feedback on specific pages of your documentation"
  },
  "autoload": {
    "psr-4": {
      "Vulcan\\UserDocs\\": "src/",
      "Vulcan\\UserDocs\\Tests\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    },
    "expose": [
      "css",
      "js"
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
